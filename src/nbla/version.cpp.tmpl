#include <nbla/common.hpp>

#include <string>

namespace nbla {

using std::string;

const string nbla_version(void) {
  static const string version("${version}");
  return version;
}

const string nbla_author(void) {
  static const string author("Takuya Narihira, Sony Corporation");
  return author;
}

const string nbla_author_email(void) {
  static const string author_email("nnabla@googlegroups.com");
  return author_email;
}

const string nbla_build_number(void) {
  static const string build_number("${build_number}");
  return build_number;
}
}
